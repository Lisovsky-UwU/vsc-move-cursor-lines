{
  "name": "move-cursor-lines",
  "displayName": "Move Cursor Lines",
  "description": "Allows you to move the cursor relatively along the rows",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Keymaps",
    "Visualization"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "moveCursorLines.move",
        "title": "Move Cursor Lines: Move"
      }
    ],
    "keybindings": [
      {
        "command": "moveCursorLines.move",
        "key": "ctrl+g",
        "mac": "cmd+g"
      }
    ],
    "configuration": {
      "title": "Move Cursor Lines",
      "properties": {
        "moveCursorLines.changeColumn": {
          "title": "Changing the cursor column",
          "description": "Determines which column the cursor will be placed on after the row is changed",
          "type": "string",
          "enum": [
            "start",
            "end",
            "save"
          ],
          "default": "start"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
